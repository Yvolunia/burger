

 <div id="burger_image"> 
          <img width=200 height=200 src="./public/assets/images/burger.jpg">
          
</div>

<h1>Eat-Da-Burger!</h1>

<ol>
  {{#each burgers}}
    {{#unless devoured}}
      {{> burgers/burgers-block devoured=false}}
    {{/unless}}
  {{/each}}
</ol>

<ul>
  {{#each burgerss}}
    {{#if devoured}}
      {{> burgers/burgers-block devoured=true}}
    {{/if}}
  {{/each}}
</ul>

<form class="create-form">

  <div class="form-group">
    <input type="text" id="ca" name="burger_name">
  </div>

  <button type="submit">Submit</button>
</form>
